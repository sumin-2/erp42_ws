<?xml version="1.0"?>
<robot name="erp42" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:arg name="pkg_share" default=""/>
  <xacro:arg name="controllers_file" default=""/>

  <xacro:include filename="$(arg pkg_share)/urdf/macros_erp42.xacro"/>
  <xacro:include filename="$(arg pkg_share)/urdf/erp42_control.gazebo" />


  <link name="super_root"/>

  <joint name="super_to_base_dummy" type="fixed">
    <parent link="super_root"/>
    <child link="base_dummy"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  <link name="base_dummy">
    <inertial>
      <mass value="160"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="20.43" ixy="0.584" ixz="-0.191" iyy="54.197" iyz="-0.2449" izz="72.2588"/>
    </inertial>
  </link>
  <joint name="base_dummy_to_structure" type="fixed">
    <parent link="base_dummy"/>
    <child link="structure"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="structure">
    <visual name="visual">
      <origin xyz="0 0 -0.03" rpy="0 0 3.14" />
      <geometry>
        <mesh filename="$(arg pkg_share)/meshes/erp42_structure.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="$(arg pkg_share)/meshes/erp42_structure.dae" scale="1 1 1"/>
      </geometry>
      <origin xyz="0 0 -0.03" rpy="0 0 3.14" />
    </collision>
    <!-- removed inertia here; it's on base_dummy -->
  </link>

  <!-- steering links with visual for debug -->
  <link name="lf_front_steer_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.02"/>
      </geometry>
      <material name="steer_green">
        <color rgba="0 1 0 0.5"/>
      </material>
    </visual>
  </link>
  <link name="rf_front_steer_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.02"/>
      </geometry>
      <material name="steer_blue">
        <color rgba="0 0 1 0.5"/>
      </material>
    </visual>
  </link>

  <!-- steering hinge joints -->
  <joint name="lf_steering_hinge_joint" type="revolute">
    <parent link="structure"/>
    <child link="lf_front_steer_link"/>
    <origin xyz="0.53 0.51 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.52" upper="0.52" effort="5.0" velocity="1.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <joint name="rf_steering_hinge_joint" type="revolute">
    <parent link="structure"/>
    <child link="rf_front_steer_link"/>
    <origin xyz="0.53 -0.51 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.52" upper="0.52" effort="5.0" velocity="1.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- front wheels (under steer links) -->
  <xacro:wheel prefix="right" suffix="front" x="0" y="0" z="0" pitch="0" yaw="1.5707"/>
  <xacro:wheel prefix="left" suffix="front" x="0" y="0" z="0" pitch="0" yaw="-1.5707"/>

  <xacro:wheel_joint prefix="right" suffix="front" parent_link="rf_front_steer_link" x="0" y="0" z="0"/>
  <xacro:wheel_joint prefix="left" suffix="front" parent_link="lf_front_steer_link" x="0" y="0" z="0"/>

  <!-- rear disk wheels and driven wheels -->
  <xacro:disk_wheel prefix="right" suffix="back" roll="0" pitch="0" yaw="1.5707"/>
  <xacro:disk_wheel prefix="left" suffix="back" roll="0" pitch="0" yaw="-1.5707" with_gazebo="true"/>

  <xacro:back_disk_wheel_joint prefix="right" suffix="back" x="-0.49" y="0.51" z="0"/>
  <xacro:back_disk_wheel_joint prefix="left" suffix="back" x="-0.49" y="-0.51" z="0"/>

  <xacro:wheel prefix="right" suffix="back" x="0" y="0" z="0" pitch="0" yaw="1.5707"/>
  <xacro:wheel prefix="left" suffix="back" x="0" y="0" z="0" pitch="0" yaw="-1.5707"/>

  <xacro:wheel_joint prefix="right" suffix="back" parent_link="right_back_disk_wheel" x="0" y="0" z="0"/>
  <xacro:wheel_joint prefix="left" suffix="back" parent_link="left_back_disk_wheel" x="0" y="0" z="0"/>

  <!-- cameras -->
  <xacro:my_camera prefix="middle"/>
  <xacro:my_camera prefix="right"/>
  <xacro:my_camera prefix="left"/>
  <xacro:my_camera prefix="back"/>
  <xacro:camera_joint prefix="middle" x="1.0" y="0" z="0.3" roll="0" pitch="0" yaw="0"/>
  <xacro:camera_joint prefix="left" x="0.05" y="0.10" z="0.3" roll="0" pitch="0" yaw="0.785"/>
  <xacro:camera_joint prefix="right" x="0.05" y="-0.10" z="0.3" roll="0" pitch="0" yaw="-0.785"/>
  <xacro:camera_joint prefix="back" x="-0.05" y="0" z="0.4" roll="0" pitch="0" yaw="3.1414"/>


  <!-- camera plugins -->
  <gazebo reference="left_camera">
    <sensor type="camera" name="left_camera1">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="left_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>30.0</updateRate>
        <cameraName>/erp42/left_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>left_camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="right_camera">
    <sensor type="camera" name="right_camera2">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="right_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>30.0</updateRate>
        <cameraName>/erp42/right_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>right_camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="middle_camera">
    <sensor type="camera" name="middle_camera3">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>          
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="middle_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>30.0</updateRate>
        <cameraName>/erp42/middle_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>middle_camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="back_camera">
    <sensor type="camera" name="back_camera4">
      <update_rate>30.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="back_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>30.0</updateRate>
        <cameraName>/erp42/back_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>back_camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>

  <link name="imu_link">
    <inertial>
      <mass value="0.001"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.00001" ixy="0.0" ixz="0.0"
              iyy="0.00001" iyz="0.0"
              izz="0.00001"/>
    </inertial>
    <visual>
      <geometry><sphere radius="0.02"/></geometry>
    </visual>
    <collision>
      <geometry><sphere radius="0.02"/></geometry>
    </collision>
  </link>
  <joint name="imu_joint" type="fixed">
    <parent link="structure"/>
    <child link="imu_link"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
  </joint>
  <gazebo reference="imu_link">
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>20.0</update_rate>
      <imu>
        <topic>imu</topic>
        <gaussian_noise>0.0</gaussian_noise>
      </imu>
      <plugin name="imu_plugin" filename="libgazebo_ros_imu_sensor.so"/>
    </sensor>
  </gazebo>
</robot>